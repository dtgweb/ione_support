<%= ext_grid_tag "support_calendar_events_grid", :ds => lambda { "Ext.StoreMgr.get('support_calendar_events')" },
  :actions => [
    lambda { ext_action("New", ext_confirm_changes_function("function() { #{ext_remote_function :url => "#{new_support_calendar_event_path}.js"} }"), {'icon_cls' => 'add'}) }, 
    lambda { ext_edit_record_action(objects_path) }, 
    lambda { ext_delete_record_action("Event", objects_path) }
  ]
%>