<%= ext_grid_tag "support_calendar_events_grid", :ds => lambda { "Ext.StoreMgr.get('support_calendar_events')" },
  :actions => [
    lambda { ext_new_record_action("#{new_support_calendar_event_path}.js", :id => "new_support_calendar_event_action", :loading => true) }, 
    lambda { ext_edit_record_action(objects_path, :id => "edit_support_calendar_event_action", :loading => true) },
    lambda { ext_delete_record_action("Event", objects_path) }
  ]
%>